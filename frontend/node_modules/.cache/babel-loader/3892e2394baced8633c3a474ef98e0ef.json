e.auth.EmailAuthProvider.PROVIDER_ID,\n              ],\n              privacyPolicyUrl: '/Privacy'\n          }\n  \n          ui.start(\"#firebaseui-auth-container\", uiConfig)\n      },\n\n      methods: {\n        Privacy: function() {\n            this.$router.push(\"/Privacy\")\n        },\n        AboutBQ: function() {\n            this.$router.push(\"/AboutUs\")\n        }\n    }\n  }\n  </script>\n  \n<style scoped>\n#firebaseui-auth-container{\n    margin-top: 50px;\n    margin-bottom: 50px;\n}\n\n.login {\n    position: fixed;\n    top: 50%;\n    left: 50%;\n    -webkit-transform: translate(-50%, -50%);\n    transform: translate(-50%, -50%);\n    background-color:white;\n    display: flex;\n    align-items: center;\n    text-align: center;\n    border: 1px solid rgb(197, 197, 197);\n    box-shadow: -5px 12px 15px rgba(0, 0, 0, 0.2), 5px -5px 15px rgba(0, 0, 0, 0.19);\n\n}\n.img{\n    width: 48%;\n    float: left;\n}\n#login_img {\n    margin: 1% 1% 0% 1%;\n    width: 100%;\n    height: 500%;\n}\n.text{\n    float: right;\n    width: 48%;\n    text-align: center;\n}\n\n#firebaseui-auth-container {\n    text-align: left;\n}\n\n#logo {\n    width: 200px;\n    height: 140px;\n}\n\n.text a:visited {\n  color: blue;\n}\n\n/* mouse over link */\n.text a:hover {\n  color: rgb(255,94,0);\n}\n\n/* selected link */\n.text a:active {\n  color: rgba(238, 119, 1, 0.5);\n}\n</style>"]}]}